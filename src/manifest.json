{
  "manifest_version": 2,
  "name": "isbn-collector",
  "version": "1.0.0",
  "icons": {
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["tabs", "storage"]
}
