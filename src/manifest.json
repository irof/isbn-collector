{
  "manifest_version": 2,
  "name": "ISBNこれくたー",
  "description": "ページに表示されたISBNをメモしておきます。",
  "version": "1.3.0",
  "icons": {
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["tabs", "storage"]
}
